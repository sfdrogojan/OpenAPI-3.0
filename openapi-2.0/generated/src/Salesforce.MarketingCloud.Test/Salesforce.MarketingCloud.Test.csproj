<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net45</TargetFramework>
    <AssemblyName>Salesforce.MarketingCloud.Test</AssemblyName>
    <PackageId>Salesforce.MarketingCloud.Test</PackageId>
    <OutputType>Library</OutputType>
    <Authors>Salesforce</Authors>
    <Company>Salesforce</Company>
    <AssemblyTitle>Salesforce MarketingCloud C# SDK</AssemblyTitle>
    <Description>The Salesforce Marketing Cloud C# SDK</Description>
    <Copyright>Copyright © Salesforce 2019</Copyright>
    <GenerateRuntimeConfigurationFiles>true</GenerateRuntimeConfigurationFiles>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <GenerateBindingRedirectsOutputType>true</GenerateBindingRedirectsOutputType>
    <RootNamespace>Salesforce.MarketingCloud.Test</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="NUnit" Version="2.6.4" />
    <PackageReference Include="RestSharp" Version="105.1.0" />
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
    <PackageReference Include="JsonSubTypes" Version="1.2.0" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="System" />
    <Reference Include="System.Core" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.ComponentModel.DataAnnotations" />
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Data" />
    <Reference Include="System.Runtime.Serialization" />
    <Reference Include="System.Xml" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Salesforce.MarketingCloud\Salesforce.MarketingCloud.csproj" />
  </ItemGroup>

</Project>